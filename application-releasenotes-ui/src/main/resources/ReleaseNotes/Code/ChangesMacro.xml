    <property>
      <code>{{velocity}}
  #macro (addClause $name $listAsString $topSpace $xwql)
    #set ($clauses = [])
    #foreach ($item in $stringtool.split($listAsString, ','))
      #set ($discard = $clauses.add("changes.${name} like :${name}${velocityCount}"))
    #end
    #set ($xwql = "$xwql (${stringtool.join($clauses, ' or ')})")
  #end
  #macro (addBinding $name $listAsString $query)
    #foreach ($item in $stringtool.split($listAsString, ','))
      #set ($query = $query.bindValue("${name}${velocityCount}", $item.trim()))
    #end
  #end
  #set ($displayer = $xcontext.macro.params.displayer)
  #set ($columns = $numbertool.toNumber($xcontext.macro.params.columns))
  #set ($audience = $xcontext.macro.params.audience)
  ## Since audience is saved as "user", "developer", "administrator" in the DB and to make it simple for users we support uppercase
  #set ($audience = $audience.toLowerCase())
  #set ($products = $xcontext.macro.params.products)
  #set ($versions = $xcontext.macro.params.versions)
  #set ($categories = $xcontext.macro.params.categories)
  #set ($importance = $xcontext.macro.params.importance)
  ## Since importance is saved as 0, 1, 2 in the DB and to make it simple for users, we support High, Medium, low too. We also support uppercase.
  #set ($importance = $importance.toLowerCase())
  #set ($importance = $importance.replaceAll('high', 2))
  #set ($importance = $importance.replaceAll('medium', 1))
  #set ($importance = $importance.replaceAll('low', 0))
  #set ($containsScreenshots = $xcontext.macro.params.containsScreenshots)
  #set ($displayEditLink = $xcontext.macro.params.displayEditLink)
  ## Find all Release Notes Changes matching the passed parameters
  #set ($topSpace = $xcontext.macro.doc.documentReference.extractFirstReference('SPACE').getName())
  #set ($xwql = "from doc.object(${topSpace}.Code.ChangesClass) as changes where ")
  #addClause("product", $products, $topSpace, $xwql)
  #addClause("audience", $audience, $topSpace, "$xwql and ")
  #addClause("version", $versions, $topSpace, "$xwql and ")
  #addClause("category", $categories, $topSpace, "$xwql and ")
  #addClause("importance", $importance, $topSpace, "$xwql and ")
  #if ("$!containsScreenshots" == 'true')
    #set ($xwql = "$xwql and (changes.screenshots &lt;&gt; '' or (changes.screenshots is not null and '' is null))")
  #elseif ("$!containsScreenshots" == 'false')
    #set ($xwql = "$xwql and not (changes.screenshots &lt;&gt; '' or (changes.screenshots is not null and '' is null))")
  #end
  #set ($query = $services.query.xwql("$xwql order by changes.importance desc"))
  #addBinding("product", $products, $query)
  #addBinding("audience", $audience, $query)
  #addBinding("version", $versions, $query)
  #addBinding("category", $categories, $query)
  #addBinding("importance", $importance, $query)
  #set ($changeItems = $query.execute())
  #if (!$changeItems.isEmpty())
    {{include reference="${topSpace}.Code.ChangesDisplayer${stringtool.capitalize($displayer)}"/}}
  #else
    No changes!
  #end
{{/velocity}}
